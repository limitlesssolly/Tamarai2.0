<!DOCTYPE html>
<html>

<head>
    <title>Products</title>
</head>

<body>
    <h1>Products</h1>
    <ul>
        <% products.forEach((product) => { %>
            <li>
                <a href="/products/<%= product._id %>">
                    <%= product.name %>
                        ,
                        <%= product.price %>
                </a>
            </li>
            <% }); %>
    </ul>
</body>

</html>