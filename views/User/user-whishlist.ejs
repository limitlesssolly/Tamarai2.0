    <div class="list-Container">
        <% if (users.length > 0) { %>
        <div class="Header">
           
            <h5 class="Heading">wishList</h5>
            <h5 class="Action">Remove all</h5>
        </div>
        <% users.forEach(user => { %>
        <div class="the_first_list_items">
            <div class="the_first_image_box">
                <!-- <img src="photos/wedding dress.jpg" alt="weddingdress" style="height: 120px;" /> -->
            </div>
            <div class="about_the_item">
                <h1 class="title"><%= user.weddingdress %></h1>
                <h3 class="subtitle"><%= user.designerAmitabbatchan %></h3>
                <!-- <img src="photos/designer.jpg" alt="designer" style="height: 30px;"> -->
            </div>
            <div class="details"></div>
            <div class="prices"></div>
            <div class="counter">
                <div class="btn"><u>user.Un Available</u></div>
            </div>
             
            <div class="prices">
                <div class="amount"><%= user.350 %> EG</div>
                <% if (users.length > 0) { %>
                    <div class="add"><u>add to shopping cart</u></div>
                    <div class="remove"><u>Remove</u></div>
                <% } %>
            </div>

        </div>
        <% }) %>
        <% } else { %>
            <h2>No whishes to display</h2>
        <% } %>



