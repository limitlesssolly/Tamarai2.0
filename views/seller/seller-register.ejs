<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Seller Sign Up</title>
    <link rel="stylesheet" href="/css/seller-signup.css">
  </head>
  <body>
    
    <div class="signupFrm">
        <form action="/seller/register" method="post" class="form" method="post">
          <h1 class="title">Sign up</h1>
    
          <div class="inputContainer">
            <input type="text" class="input" placeholder="a" name="email" id="emailInput">
            <label for="" class="label">Email</label>
          </div>
          
          <div class="inputContainer">
            <input type="text" class="input" placeholder="a" name="username" id="usernameInput">
            <label for="" class="label">Username</label>
          </div>
          
          <div class="inputContainer">
            <input type="password" class="input" placeholder="a" name="password" id="passwordInput">
            <label for="" class="label">Password</label>
          </div>
          
          <div class="inputContainer">
            <input type="password" class="input" placeholder="a" id="confirmPasswordInput">
            <label for="" class="label">Confirm Password</label>
          </div>

    
          <input type="submit" class="submitBtn" value="Sign up">
        </form>
      </div>
      <%- include('../partials/footer.ejs') %>
  </body>
  <script src="/JS/valido.js"></script>
</html>